<launch>
<!--node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true"-->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" clear_params="true">
    <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />

    <rosparam file="$(find my_navigation)/config/teb/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find my_navigation)/config/teb/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find my_navigation)/config/teb/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find my_navigation)/config/teb/global_costmap_params.yaml" command="load" />

    <rosparam file="$(find my_navigation)/config/teb/move_base_params.yaml" command="load" />
    <rosparam file="$(find my_navigation)/config/teb/base_global_planner_params.yaml" command="load" />
    <rosparam file="$(find my_navigation)/config/teb/teb_local_planner_params.yaml" command="load" />
    <remap from="/cmd_vel" to="/teb/cmd_vel"/>  
  </node>
  
</launch>
